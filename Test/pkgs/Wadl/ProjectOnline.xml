  <resources siena:authenticationProviderHref="#PowerAppAuth" base="https://brazil-001.azure-apim.net/apim/projectonline">
    <resource path="/{connectionId}/_api/ProjectServer/Projects">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="GET" id="ListProjects" siena:isDeprecated="false">
        <doc title="Listar projetos&#xA;Lista os projetos no seu site do Project Online." />
        <request>
          <param style="query" name="siteUrl" type="xs:anyURI" siena:dtype="hyperlink" required="true">
            <doc title="URL do site raiz do site do seu projeto (Exemplo: https://sampletenant.sharepoint.com/teams/sampleteam)." />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:ProjectsWrapper" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/_api/ProjectServer/Projects">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="POST" id="CreateProject" siena:isDeprecated="false">
        <doc title="Cria novo projeto&#xA;Cria um novo projeto no seu site do Project Online." />
        <request>
          <param style="query" name="siteUrl" type="xs:anyURI" siena:dtype="hyperlink" required="true">
            <doc title="URL do site raiz do site do seu projeto (Exemplo: https://sampletenant.sharepoint.com/teams/sampleteam)." />
          </param>
          <representation mediaType="application/json">
            <param style="plain" name="Name" path="/Name" type="xs:string" required="true" />
            <param style="plain" name="Description" path="/Description" type="xs:string" />
            <param style="plain" name="Start" path="/Start" type="service:NewProject_Start_def" />
          </representation>
        </request>
        <response>
          <representation mediaType="application/json" element="service:Project" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/_api/ProjectServer/Projects('{project_id}')">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <param style="template" name="project_id" type="xs:string" required="true">
        <doc title="A ID exclusiva do projeto a se buscar" />
      </param>
      <method siena:requiresAuthentication="true" name="GET" id="ListProject" siena:isDeprecated="false">
        <doc title="Listar projeto&#xA;Lista um projeto pela ID no site do Project Online." />
        <request>
          <param style="query" name="siteUrl" type="xs:anyURI" siena:dtype="hyperlink" required="true">
            <doc title="URL do site raiz do site do seu projeto (Exemplo: https://sampletenant.sharepoint.com/teams/sampleteam)." />
          </param>
          <param style="query" name="$select" type="xs:string">
            <doc title="Consulta selecionada pelo OData para as colunas a serem retornadas. Exemplo: Id, Name" />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:Project" />
        </response>
      </method>
    </resource>
